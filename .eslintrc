{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-dangle": [2, "always-multiline"],
    "max-len": [2, 120, 4],
    "max-params": [1, 4],
    "no-extra-parens": [0, "all"],
    "max-statements": [1, 25],
    "callback-return": [0],
    "prefer-spread": [0]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": false
  },
  "extends": [
    "defaults/configurations/walmart/es6-node",
    "defaults/rules/filenames/off"
  ]
}